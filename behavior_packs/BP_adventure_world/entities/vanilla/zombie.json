                "minecraft:movement": { "value": 0.25 },
				"minecraft:equipment": { "table": "loot_tables/equipments/armor/netherite_armor.json" }
            },

			"have_no_shield": {
                "minecraft:timer": { "time": [ 2, 4 ], "looping": true, "time_down_event": { "event": "aw:have_shield", "target": "self" } }
            },
            "have_weak_shield": {
                "minecraft:damage_sensor": {
                    "triggers": [
                        { "on_damage": { "filters": { "any_of": [ { "none_of": { "test": "is_family", "value": "player", "subject": "other" } }, { "none_of": { "test": "has_equipment", "domain": "hand", "value": "aw:wild_sword", "subject": "other" } } ] } }, "deals_damage": false },
                        { "on_damage": { "filters": { "all_of": [ { "test": "is_family", "value": "player", "subject": "other" }, { "test": "has_equipment", "domain": "hand", "value": "aw:wild_sword", "subject": "other" } ] }, "target": "self", "event": "aw:have_no_shield" } }
                    ]
                }
            },
            "have_shield": {
                "minecraft:damage_sensor": { "triggers": [ { "cause": "all", "deals_damage": false } ] }
            }
		}
	}
}