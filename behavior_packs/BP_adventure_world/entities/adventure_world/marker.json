{
    "format_version": "1.21.0",
    "minecraft:entity": {
        "description": {
            "identifier": "aw:marker",
            "is_experimental": false,
            "is_spawnable": false,
            "is_summonable": true,
            "properties": {
                "aw:is_level_marker": { "type": "bool", "default": false },
                "aw:level": { "type": "enum", "values": [ "this", "prev", "next" ], "default": "this" }
            }
        },
        "components": {
            "minecraft:physics": { "has_collision": false, "has_gravity": false },
            "minecraft:collision_box": { "height": 0, "width": 0 },
            "minecraft:type_family": { "family": [ "inanimate", "marker" ] },
            "minecraft:persistent": { },
            "minecraft:nameable": { },
            "minecraft:damage_sensor": { "triggers": [ { "cause": "all", "deals_damage": false } ] }
        },
        "events": {
            "aw:set_this_level": { "set_property": { "aw:is_level_marker": true, "aw:level": "this" } },
            "aw:set_prev_level": { "set_property": { "aw:is_level_marker": true, "aw:level": "prev" } },
            "aw:set_next_level": { "set_property": { "aw:is_level_marker": true, "aw:level": "next" } },
            "aw:remove_immediately": { "add": { "component_groups": [ "remove_immediately" ] } }
        },
        "component_groups": {
            "remove_immediately": {
                "minecraft:instant_despawn": { }
            }
        }
    }
}