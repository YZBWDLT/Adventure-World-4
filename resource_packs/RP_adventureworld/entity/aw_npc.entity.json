{
    "format_version": "1.10.0",
    "minecraft:client_entity": {
        "description": {
            "identifier": "aw:npc",
            "materials": { "default": "entity_alphatest" },
            "textures": {
                "npc_0": "textures/new_entities/npc/0",
                "npc_1": "textures/new_entities/npc/1",
                "npc_2": "textures/new_entities/npc/2",
                "npc_3": "textures/new_entities/npc/3",
                "miyunxi": "textures/new_entities/npc/miyunxi",
                "linan": "textures/new_entities/npc/linan",
                "zhangyu": "textures/new_entities/npc/zhangyu",
                "tianying": "textures/new_entities/npc/tianying"
            },
            "geometry": {
                "male": "geometry.manikin_male",
                "female": "geometry.manikin_female"
            },
            "render_controllers": [ "controller.render.aw_npc" ],
            "spawn_egg": { "base_color": "#e8b211", "overlay_color": "#47f118" },
            "animations": {
                "bob": "animation.player.bob",
                "look_at_target_default": "animation.humanoid.look_at_target.default",
                "move": "animation.humanoid.move",

                "controller_move": "controller.animation.humanoid.move"
            },
            "scripts": {
                "scale": "0.9375",
                "animate": [
                    "bob", "look_at_target_default", "controller_move"
                ],
                "pre_animation": [
                    "variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;"
                ]
            }
        }
    }
}