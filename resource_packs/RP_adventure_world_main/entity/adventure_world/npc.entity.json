                "scale": "0.9375",
                "pre_animation": [
                    "variable.geometry = query.property('aw:geometry') == 'slim' ? 1 : 0;",
                    "variable.skin = query.property('aw:skin');",
                    
                    "variable.state = query.property('aw:state');",
                    "variable.is_sitting = variable.state == 'sit' || variable.state == 'sit_raising_head';",
                    "variable.is_walking = query.is_moving;",
                    "variable.anim = query.property('aw:animation');",
                    "variable.speak_mode = query.property('aw:speak_mode');"
                ],
                "animate": [
                    "state_controller",
                    "animation_controller"
                ]
            },
            "animations": {
                "state_controller": "controller.animation.aw_npc.state",
